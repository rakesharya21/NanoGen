#!/home/proj/ug/15/ugarya/rakesh/projects/Nanogen/ng_venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pymatgen==2018.8.10','console_scripts','pydii'
__requires__ = 'pymatgen==2018.8.10'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pymatgen==2018.8.10', 'console_scripts', 'pydii')()
    )
